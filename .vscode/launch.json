  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}",
      "env": {
        "CGO_ENABLED": "1"
      }
    },
    {
      "name": "Launch Package (WSL)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {
        "CGO_ENABLED": "1"
      }
    }
  ]
}
